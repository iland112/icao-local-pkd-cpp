@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find dependencies
find_dependency(spdlog REQUIRED)

# Find OpenLDAP
find_path(LDAP_INCLUDE_DIR ldap.h)
find_library(LDAP_LIBRARY NAMES ldap)
find_library(LBER_LIBRARY NAMES lber)

# Include targets
if(NOT TARGET icao::ldap)
    include("${CMAKE_CURRENT_LIST_DIR}/icao-ldap-targets.cmake")
endif()

check_required_components(icao-ldap)
